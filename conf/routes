# Home page
GET        /                                           @controllers.Application.index
GET        /handleInstagramToken/inputTag              @controllers.TagSearch.inputTag()
GET        /handleInstagramToken/findPhotoByTag        @controllers.TagSearch.findPhotoByTag(tagName: String)
GET        /handleInstagramToken/                      @controllers.OAuther.createInsta(code: String)

# Exploring jInstagram
GET        /home                                       @controllers.Admin.adminPage

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                               controllers.Assets.at(path="/public", file)
