# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                                 controllers.Application.index
#GET		/handleInstagramToken/		controllers.TagSearch.test(code:String)

#GET        /handleInstagramToken/            controllers.Data.test(code:String)
GET        /handleInstagramToken/            controllers.Explorer.showInsta(code: String)
GET        /handleInstagramToken/post        controllers.Data.post()
GET        /test                             controllers.Explorer.some

# Exploring jInstagram
GET        /media                            controllers.Explorer.showMediaFeed(mediaId = "5656")
GET        /admin                            controllers.Admin.adminPage

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                     controllers.Assets.at(path="/public", file)
